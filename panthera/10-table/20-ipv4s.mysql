CREATE TABLE `ipv4s` (
    `id` BIGINT UNSIGNED NOT NULL AUTO_INCREMENT,
    `a` TINYINT UNSIGNED DEFAULT NULL,
    `b` TINYINT UNSIGNED DEFAULT NULL,
    `c` TINYINT UNSIGNED DEFAULT NULL,
    `d` TINYINT UNSIGNED DEFAULT NULL,
     UNIQUE KEY `id` (`id`),
     KEY `index` (`id`)
)  ENGINE=INNODB DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci;
